<div class="col-md-6">
    <div class="card">
        <div class="card-header">
            <h5>Precios de Diesel</h5>
        </div>
        <div class="card-block">
           <div class="table-responsive">
            <table class="table">
              <thead>
                <tr>
                  <th>Cantidad</th>
                  <th>Fecha</th>
                  <th colspan="3"></th>
                </tr>
              </thead>

              <tbody>
                <% @gas.each do |ga| %>
                <tr>
                  <td><i class="feather icon-droplet"></i> <%= ga.try(:cantidad) %></td>
                  <td><i class="feather icon-calendar"></i> <%= ga.try(:month2) %></td>
                  <td><%= link_to 'Editar', edit_ga_path(ga) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>       
        </div>
    </div>
</div>
