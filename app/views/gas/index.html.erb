<p id="notice"><%= notice %></p>


<div id="formario">
<h1>Precios de Diesel</h1>

      <div class="content table-responsive table-full-width">
        <table class="table table-hover table-striped">
  <thead>
    <tr>
      <th>Cantidad</th>
      <th>Fecha</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gas.each do |ga| %>
      <tr>
        <td><%= ga.cantidad %></td>
        <td><%= ga.month %></td>
        <td><%= link_to 'Editar', edit_ga_path(ga) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
</div>

<br>
