<div class="row">
  <div class="col-md-12">
    <div class="panel panel-headline">
      <div class="panel-heading">
        <h3>Gastos</h3>
        <hr>
      </div>
      <div class="panel-body">
        <div class="content table-responsive table-full-width">
          <table class="table table-striped">
  <thead>
    <tr>
      <th>Categoria</th>
      <th>Cantidad</th>
      <th>Nota</th>
      <th>Mes</th>
      <th>Ano</th>
      <th>Fecha</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @expenses.each do |expense| %>
      <tr>
        <td><%= expense.categoria %></td>
        <td><%= expense.cantidad %></td>
        <td><%= expense.nota %></td>
        <td><%= expense.mes %></td>
        <td><%= expense.ano %></td>
        <td><%= expense.fecha %></td>
        <td><%= link_to 'Ver mÃ¡s', expense %></td>
        <td><%= link_to 'Editar', edit_expense_path(expense) %></td>
        <td><%= link_to 'Eliminar', expense, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
        </div>
      </div>
    </div>
  </div>
</div>

<br>